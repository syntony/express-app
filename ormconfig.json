{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "app/models/**/*.ts"
   ],
   "migrations": [
      "app/migration/**/*.ts"
   ],
   "subscribers": [
      "app/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "app/models",
      "migrationsDir": "app/migration",
      "subscribersDir": "app/subscriber"
   }
}