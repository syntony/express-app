{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Awesome project developed with TypeORM.",
  "private": true,
  "main": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">=10.16.0",
    "yarn": ">=1.16.0",
    "npm": ">=6.11.3"
  },
  "scripts": {
    "build": "webpack",
    "test": "jest",
    "tsc": "tsc",
    "start": "set debug=* && ts-node-dev --respawn --transpileOnly src/index.ts",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn start\" \"yarn client\"",
    "prod": "tsc && node ./build/app.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "lint": "eslint './src/**/*.{js,ts}' --quiet --fix"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "^0.0.46",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "test": "^0.6.0",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.2",
    "ts-node": "3.3.0",
    "typescript": "2.5.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "express": "^4.15.4",
    "faker": "^4.1.0",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "sqlite3": "^4.0.3",
    "ts-node-dev": "^1.0.0-pre.32",
    "typeorm": "0.2.12"
  }
}
